// This tells Babel:
// "When compiling JSX, use createElement() (not React) and output normal JS."

{
    "presets": [
        ["@babel/preset-react", {
            "pragma": "createElement",
            "runtime": "classic",
            "pragmaFrag": "Fragment"
        }],
        ["@babel/preset-env", {
            "modules": false
        }]
    ]
}